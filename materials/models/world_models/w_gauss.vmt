"VertexLitGeneric"
{
	"$baseTexture" "models\world_models/w_gauss"
	"$bumpmap" "models\world_models/w_gauss_n"
    "$normalmapalphaenvmapmask" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.8 0.8 0.8]"

 $phong 1
        $phongexponent 20
        $phongboost .30
//      $phongtint "[1 1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

	"$halflambert" "1"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "25"		// Exponent for phong component of rim lighting
	"$rimlightboost" "1.5"			// Boost for ambient cube component of rim lighting
}